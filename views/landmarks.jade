extends layout
block content
    .row
      .grid-container 
     
        each row in res1
         .item 
           h1.itemtitle #{row.Name}
           img.pimage(src='#{row.Image}', alt='#{row.Name}')
           p.itemp Entry Cost: â‚¬#{row.Price}
           p.itemp Country: #{row.Country}
           p.itemp Id: #{row.Id}
